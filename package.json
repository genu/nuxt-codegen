{
  "name": "nuxt-codegen",
  "description": "GraphQL Code Generator module for Nuxt 3",
  "author": "Eugen Istoc",
  "version": "1.3.1",
  "license": "MIT",
  "homepage": "https://github.com/genu/nuxt-codegen",
  "bugs": {
    "url": "https://github.com/genu/nuxt-codegen/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/genu/nuxt-codegen"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint",
    "test": "echo no tests"
  },
  "dependencies": {
    "@changesets/cli": "^2.25.2",
    "@graphql-codegen/cli": "2.16.1",
    "@nuxt/kit": "^3.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/typescript": "2.8.5",
    "@graphql-codegen/typescript-operations": "2.5.10",
    "@graphql-codegen/typescript-vue-urql": "2.3.6",
    "@graphql-codegen/urql-introspection": "2.2.1",
    "@nuxt/module-builder": "^0.2.0",
    "@nuxt/schema": "^3.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.26.0",
    "nuxt": "^3.0.0"
  }
}
